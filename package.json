{
  "name": "suppress-eslint-errors",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Faithlife/suppress-eslint-errors"
  },
  "author": "David Mitchell <dave@fallingcanbedeadly.com>",
  "license": "MIT",
  "description": "A codemod that suppresses existing violations of eslint rules.",
  "engines": {
    "node": ">= 10.12.0"
  },
  "bin": {
    "suppress-eslint-errors": "./bin/index.js"
  },
  "files": [
    "bin/index.js",
    "transforms/*.js"
  ],
  "scripts": {
    "suppress-eslint-errors": "node bin/index.js",
    "lint": "eslint bin transforms",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "faithlife"
    ]
  },
  "release": {
    "branch": "master"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cross-spawn": "^7.0.3",
    "jscodeshift": "^0.11.0",
    "please-upgrade-node": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "commitlint-config-faithlife": "^1.0.0",
    "eslint": "^7.17.0",
    "eslint-config-faithlife": "^8.1.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.2",
    "typescript": "^4.1.3"
  }
}
